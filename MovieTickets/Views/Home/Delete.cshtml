@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model MovieTickets.Models.Movie
<h2>Удаление фильма</h2>
<dl>
    <dt>Название</dt>
    <dd>
        @Html.DisplayFor(m => m.Name)
    </dd>

    <dt>Цена</dt>
    <dd>
        @Html.DisplayFor(m => m.Price)
    </dd>
</dl>

@using (Html.BeginForm())
{
    <input type="submit" value="Удалить" />
}

